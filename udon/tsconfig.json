{
  "compilerOptions": {
    "baseUrl": "./src",
      "paths": {
        "@pages": ["views/pages/index.ts"],
        "@pages/*": ["views/pages/*"],
        "@components": ["views/components/index.ts"],
        "@components/*": ["views/components/*"],
        "@groupdetail": ["views/components/groupdetail/index.ts"],
        "@groupdetail/*": ["views/components/groupdetail/*"],
        "@styles/*": ["views/styles/*"],
      },
      
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src"],
}
